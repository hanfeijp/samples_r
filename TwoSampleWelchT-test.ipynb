   "source": [
    "# irisデータセット\n",
    "head(iris)\n",
    "d1 <- subset(iris, Species==\"setosa\")$Petal.Length # setosaのPetal.Length\n",
    "d2 <- subset(iris, Species==\"virginica\")$Petal.Length # virginicaのPetal.Length\n",
    "head(d1)\n",
    "head(d2)"
   ]
